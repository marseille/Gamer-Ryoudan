<h3 class=red_list_header> Campaigns </h3>
<table class="hor-minimalist-a">
  <thead>
    <%= render :partial => "table_headers" %>
  </thead>
  <tbody>
    <% if @currently_playing.length > 0 %>
      <% @currently_playing.each do |game|%>
        <%= render :partial => "table_content", :locals => {:game => game}%>
      <% end %>
    <% end %>
  </tbody>
</table>
<br />