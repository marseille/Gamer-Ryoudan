<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
"http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
	<meta http-equiv="content-type" content="text/html; charset=UTF-8"/>
	<%= stylesheet_link_tag "City_nightlife/images/style.css" %>
</head>
	<div id="wrap">
		<div id="container">
			<div id="spinner"></div>      
      <% @user = current_user %>      
			<div id="sidebar"> </div>
			<div id="main">
        <% if flash[:notice] %>
          <%= flash[:notice] %>
        <% end %>
				<div id="flash"></div>          
          <% if @user %>
            <%= link_to "logout", :controller => "user_sessions", :action => "destroy" %>            
            <%= label(:current_login, "(currently logged in as: " + @user.login + ")") %>
            <%= link_to " my profile", account_url %>
            <br><br>
            <div id="error_messages"></div>
          <% end %>
          
